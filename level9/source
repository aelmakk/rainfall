#include <iostream>
#include <cstring>
#include <cstdlib>

class N
{
  public:
    char str[104];
    int nbr;
    N(int nbr);
    void setAnnotation(N * obj, char *str);
};

N::N(int nbr)
{
  *(int *)(this + 104) = nbr;
  return;
}

void    N::setAnnotation(N * obj, char *str)
{
  memcpy(obj, str, strlen(str));
  return;
}

int   main(int argc, char *argv[])
{
    N *obj1;
    N *obj2;

    if (argc < 2) {
      exit(1);
    }
    obj1 = new N(5);
    obj2 = new N(6);
    obj2->setAnnotation(obj1, argv[1]);
    return (0);
}