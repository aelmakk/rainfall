{
  printf("%s - %d\n", c, time(0));
}


int     main(int argc, char *argv[])
{
  void *ptr1;
  char *tmp_ptr;
  void *ptr3;
  FILE *file_ptr;
  
  ptr1 = malloc(8);
  *(ptr1) = 1;
  tmp_ptr = (char *)malloc(8);
  *(ptr1 + 4) = (char *) tmp_ptr;
  ptr3 = malloc(8);
  *(ptr3) = 2;
  tmp_ptr = (char *)malloc(8);
  *(ptr3 + 4) = tmp_ptr;
  strcpy((char *)(ptr1 + 4), argv[1]);
  strcpy((char *)(ptr3 + 4), argv[2]);
  file_ptr = fopen("/home/user/level8/.pass","r");
